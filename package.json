{
  "name": "starter-kit",
  "description": "Custom project",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.12.3",
    "css-mqpacker": "^5.0.1",
    "del": "^2.2.0",
    "eslint-config-google": "^0.7.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.4",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.0",
    "gulp-inject-file": "0.0.18",
    "gulp-load-plugins": "^1.2.2",
    "gulp-newer": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-sass": "^2.2.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uglify": "^2.0.0",
    "gulp-webp": "^2.3.0",
    "gulp-webpcss": "^1.1.1",
    "postcss-assets": "^4.0.1",
    "postcss-mq-keyframes": "^0.3.0",
    "sw-precache": "^4.2.1",
    "sw-toolbox": "^3.4.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "david": {
    "ignore": [
      "gulp"
    ]
  }
}
